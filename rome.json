{
	"$schema": "./node_modules/rome/configuration_schema.json",
	"linter": {
		"enabled": true,
		"ignore": [
			"node_modules/",
			"coverage/",
			"dist/"
		],
		"rules": {
			"recommended": true,
			"correctness": {
				"noRenderReturnValue": "off"
			},
			"suspicious": {
				"noArrayIndexKey": "off"
			}
		}
	},
	"formatter": {
		"enabled": true,
		"ignore": [
			"node_modules/",
			"coverage/",
			"dist/"
		]
	}
}